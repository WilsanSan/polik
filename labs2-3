#include <math.h>
#include <iostream>
using namespace std;
//1
int main()
{
	double a, b, res;
	cin >> a >> b;
	res = sqrt((a * b) / (a / b));
	cout << res << endl;

	//2
	cin >> a >> b;
	if (a > b) {
		res = (a / b);
		cout << res << endl;
	}
	else {
		res = (a * b);
	cout << res << endl;
	}

	//3
	int i,s=0;
	cin >> i >> a >> b;
	while (a > i)
	{
		s = s + i;
		i += 2;
		res = (a + b) / 4;
	}
	if (res > s) 
	{
		cout << s + 100 << endl;
	}
	else {
		cout << res + 100 << endl;
	}
	

}
